<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RandomStart.Pages.MenuPage">
    <ContentPage.Content>
        <TableView>
            <TableSection Title="{Binding MenuPageTitle}">
                <TextCell Text="{Binding PropertyPageText}" Command="{Binding ShowProperties}"></TextCell>
                <TextCell Text="{Binding LogPageText}" Command="{Binding ShowLog}"></TextCell>
            </TableSection>
        </TableView>
    </ContentPage.Content>
</ContentPage>